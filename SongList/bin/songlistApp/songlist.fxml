<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox 
	fillWidth="false" 
	maxHeight="400.0" 
	maxWidth="552.0" 
	minHeight="400.0" 
	minWidth="552.0" 
	prefHeight="400.0" 
	prefWidth="552.0" 
	xmlns="http://javafx.com/javafx/11.0.1" 
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="songlistApp.Controller">
	
  <children>
    <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="548.0" VBox.vgrow="ALWAYS">
      <children>
            
            <ListView fx:id="listView" layoutX="25.0" layoutY="28.0" prefHeight="325.0" prefWidth="279.0" />
            
            <Button fx:id="add" onAction="#addPress" layoutX="345.0" layoutY="29.0" mnemonicParsing="false" text="Add" />
            <Button fx:id="edit" onAction="#editPress" layoutX="400.0" layoutY="29.0" mnemonicParsing="false" text="Edit" />
            <Button fx:id="delete" onAction="#deletePress" layoutX="455.0" layoutY="29.0" mnemonicParsing="false" text="Delete" />
            
			<TextField  fx:id="songField" disable="true" layoutX="345.0" layoutY="119.0" visible="false" />
            <Text fx:id="songText" disable="true" layoutX="352.0" layoutY="109.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Song" visible="false" />
            
            <TextField  fx:id="artistField" disable="true" layoutX="345.0" layoutY="176.0" visible="false" />
            <Text fx:id="artistText" disable="true" layoutX="352.0" layoutY="166.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Artist" visible="false" />
            
            <TextField  fx:id="albumField" disable="true" layoutX="345.0" layoutY="231.0" visible="false" />
            <Text fx:id="albumText" disable="true" layoutX="352.0" layoutY="221.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Album" visible="false" />
            
            <TextField  fx:id="yearField" disable="true" layoutX="345.0" layoutY="287.0" visible="false" />
            <Text fx:id="yearText" disable="true" layoutX="352.0" layoutY="277.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Year" visible="false" />
            
            <Button fx:id="applyEdit" onAction="#applyEditPress" disable="true" layoutX="348.0" layoutY="328.0" mnemonicParsing="false" text="Apply Edit" visible="false" />
            <Button fx:id="addSong" onAction="#addSongPress" disable="true" layoutX="348.0" layoutY="328.0" mnemonicParsing="false" text="Add Song" visible="false" />
            <Button fx:id="cancel" onAction="#cancelPress" disable="true" layoutX="435.0" layoutY="328.0" mnemonicParsing="false" text="Cancel" visible="false" />
      </children>
    </AnchorPane>
  </children>
</VBox>
